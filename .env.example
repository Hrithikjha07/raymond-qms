# Raymond QMS Environment Configuration
# Copy this file to .env and update the values as needed

# Application Settings
REACT_APP_TITLE=Raymond QMS
REACT_APP_VERSION=1.0.0
REACT_APP_COMPANY_NAME=Raymond Textiles

# API Configuration
REACT_APP_API_URL=http://localhost:5000/api
REACT_APP_MOCK_API=true

# Authentication Settings
REACT_APP_AUTH_ENABLED=true
REACT_APP_AUTH_STORAGE_KEY=raymond_qms_auth

# Feature Flags
REACT_APP_FEATURE_PHOTO_UPLOAD=true
REACT_APP_FEATURE_EXPORT_REPORTS=true
REACT_APP_FEATURE_NOTIFICATIONS=false

# Customization
REACT_APP_PRIMARY_COLOR=#007bff
REACT_APP_LOGO_URL=/logo.png

# Deployment Settings
GENERATE_SOURCEMAP=false 